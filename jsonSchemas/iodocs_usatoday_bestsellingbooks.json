   "resources":{
      "Best Selling Books Methods":{
         "methods":{
            "All Best Selling Book Lists published since 1993":{
               "path":"/bestsellers/books/dates",
               "httpMethod":"GET",
               "description":"Returns the date of each weekly books list published since October 1993, when the books list began",
               "parameters":{
                  "minyear":{
                     "type":"string",
                     "required":false,
                     "default":"",
                     "description":"Use with maxyear to specify a range",
                     "location":"query"
                  },
                  "maxyear":{
                     "type":"string",
                     "required":false,
                     "default":"",
                     "description":"Use with minyear to specify a range",
                     "location":"query"
                  },
                  "encoding":{
                     "type":"string",
                     "required":false,
                     "default":"json",
                     "description":"Output format as JSON or XML",
                     "enum":[
                        "json",
                        "xml"
                     ],
                     "location":"query"
                  }
               }
            },
            "Best Selling Book Lists (For a particular a Year)":{
               "path":"/bestsellers/books/dates/year",
               "httpMethod":"GET",
               "description":"Returns the date of each weekly books list for a particular year (published since October 1993, when the books list began)",
               "parameters":{
                  "year":{
                     "type":"string",
                     "required":false,
                     "default":"2011",
                     "description":"Choose a year",
                     "location":"pathReplace"
                  },
                  "encoding":{
                     "type":"string",
                     "required":false,
                     "default":"json",
                     "description":"Output format as JSON or XML",
                     "enum":[
                        "json",
                        "xml"
